{
  "name": "@balena/katapult",
  "description": "A tool for launching container-based products",
  "version": "2.1.3",
  "author": "Michael Angelos Simos <michalis@balena.io>",
  "bin": {
    "katapult": "./bin/run"
  },
  "bugs": "https://github.com/loop-os/katapult/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.10",
    "@oclif/plugin-help": "^2.1.6",
    "@types/ssh2": "^0.5.39",
    "@types/temp": "^0.8.34",
    "ajv": "^6.10.0",
    "asn1js": "^2.0.22",
    "base32-encode": "^1.1.1",
    "bluebird": "^3.5.3",
    "dhparam": "^2.0.0",
    "dotenv": "^7.0.0",
    "inquirer": "^6.3.1",
    "js-yaml": "^3.13.1",
    "jsonschema": "^1.2.4",
    "kubernetes-client": "8.3.4",
    "lodash": "^4.17.11",
    "mustache": "^3.0.1",
    "mv": "^2.1.1",
    "mz": "^2.7.0",
    "node-forge": "^0.8.1",
    "openpgp": "^4.5.3",
    "pkijs": "^2.1.77",
    "pvutils": "^1.0.17",
    "simple-git": "^1.107.0",
    "ssh2": "^0.8.5",
    "temp": "^0.9.0",
    "tslib": "^1.9.3",
    "typed-error": "^3.1.0",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.3",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/bluebird": "^3.5.26",
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^6.1.1",
    "@types/fs-extra": "^7.0.0",
    "@types/inquirer": "^6.0.1",
    "@types/js-yaml": "^3.12.1",
    "@types/mocha": "^5.2.6",
    "@types/mustache": "^0.8.32",
    "@types/mz": "0.0.32",
    "@types/node": "^10.14.8",
    "@types/node-forge": "^0.8.3",
    "@types/openpgp": "^4.4.5",
    "@types/pkijs": "0.0.2",
    "@types/pvutils": "0.0.1",
    "@types/validator": "^10.11.0",
    "chai": "^4.2.0",
    "globby": "^8.0.2",
    "husky": "^2.2.0",
    "json-schema-to-typescript": "^6.1.3",
    "lint-staged": "^8.1.6",
    "mocha": "^5.2.0",
    "nyc": "^14.1.0",
    "prettier": "^1.17.0",
    "resin-lint": "^2.0.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/loop-os/katapult",
  "keywords": [
    "oclif"
  ],
  "license": "Apache 2.0",
  "main": "src/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "katapult",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --config ./node_modules/resin-lint/config/.prettierrc --write",
      "resin-lint --typescript --no-prettier",
      "git add"
    ]
  },
  "repository": "loop-os/katapult",
  "scripts": {
    "build": "tsc",
    "prettify": "prettier --config ./node_modules/resin-lint/config/.prettierrc --write \"src/**/*.ts\" \"typings/**/*.ts\" \"test/**/*.ts\"",
    "lint": "resin-lint --typescript src/ test/ && tsc --noEmit",
    "postpack": "rm -f oclif.manifest.json",
      "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "localtest": "./test/scripts/localtest.sh",
    "test": "mocha \"test/**/*.spec.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
